extends base.pug
   
   
block script 
      script(src='/static/index.js')

block style 
       style
         include ../static/style.css
         include ../static/stylecontact.css


block content 
      h1 hiiiii.. this is contact form
      form(action='/contact' method='post' class='form') 
            input(type='text' class='name' name='name' placeholder='Enter your name:') 
            input(type='text' class='name' name='age' placeholder='Enter your age:') 
            input(type='number' class='name' name='number' placeholder='Enter your phone number:') 
            input(type='text' class='name' name='address' placeholder='Enter your address:') 
            input(type='date' class='name' name='date' placeholder='Enter your DOB :') 
            button.btn Submit




